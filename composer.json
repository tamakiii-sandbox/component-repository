{
    "name": "concretehouse/component-repository",
    "description": "Concretes of 'concretehouse/component-repository'",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "tamakiii",
            "email": "tamaki.htm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Concretehouse\\Component\\Repository\\": "src"
        }
    },
    "require": {
        "php": ">=5.3.2",
        "concretehouse/factory-pattern": "dev-master",
        "concretehouse/repository-pattern": "dev-master",
        "pimple/pimple": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "Phake/phake": "dev-master"
    },
    "bin": [
        "bin/phpunit",
        "bin/phpunit-skeleton"
    ],
    "config": {
        "vendor-dir": "vendor"
    }
}
